<UserControl
    x:Class="appLauncher.Core.Control.MenuControl"
        xmlns:i="using:Microsoft.Xaml.Interactivity"
    xmlns:c="using:Microsoft.Xaml.Interactions.Core"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:appLauncher.Core.Control"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="60"
    d:DesignWidth="400">

    <CommandBar Height="50">
        <CommandBar.PrimaryCommands>
            <AppBarButton Content="Home" Icon="Home" />

            <AppBarButton Content="Search" Icon="Find" />
            <AppBarButton Content="Settings" Icon="Setting" />
        </CommandBar.PrimaryCommands>
        <CommandBar.SecondaryCommands>
            <AppBarButton Content="Help" Icon="Help" Visibility="Collapsed" />
            <AppBarButton Content="About" Icon="ContactInfo" Visibility="Collapsed" />
        </CommandBar.SecondaryCommands>
    </CommandBar>
</UserControl>
